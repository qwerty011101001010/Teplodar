<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2B2929"
    tools:context=".viborObject">

    <Button
        android:id="@+id/buttonHome"
        android:layout_width="177dp"
        android:layout_height="55dp"
        android:layout_marginTop="80dp"
        android:backgroundTint="@color/BGButton"
        android:text="Дом"
        android:textSize="24sp"
        app:cornerRadius="9sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/textView20"
        app:strokeColor="@color/StrokeButton"
        app:strokeWidth="1sp" />

    <Button
        android:id="@+id/buttonHome3"
        android:layout_width="177dp"
        android:layout_height="55dp"
        android:layout_marginTop="34dp"
        android:backgroundTint="@color/BGButton"
        android:text="Ангар"
        android:textSize="24sp"
        app:cornerRadius="9sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonKvartira"
        app:strokeColor="@color/StrokeButton"
        app:strokeWidth="1sp" />

    <Button
        android:id="@+id/buttonKvartira"
        android:layout_width="177dp"
        android:layout_height="55dp"
        android:layout_marginTop="34dp"
        android:backgroundTint="@color/BGButton"
        android:text="Квартира"
        android:textSize="24sp"
        app:cornerRadius="9sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonHome"
        app:strokeColor="@color/StrokeButton"
        app:strokeWidth="1sp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="163dp"
        android:text="Выбор обьекта"
        android:textColor="@color/ColorQuestion"
        android:textSize="32sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="92dp"
        android:layout_height="76dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/iconlogo" />

    <Button
        android:id="@+id/buttonBack"
        android:layout_width="177dp"
        android:layout_height="55dp"
        android:layout_marginTop="129dp"
        android:layout_marginBottom="67dp"
        android:backgroundTint="@color/BG"
        android:text="@string/ButtonBack"
        android:textColor="@color/NameButton"
        android:textSize="24sp"
        app:cornerRadius="9sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonHome3"
        app:strokeColor="@color/BGButton"
        app:strokeWidth="1sp" />

</androidx.constraintlayout.widget.ConstraintLayout>