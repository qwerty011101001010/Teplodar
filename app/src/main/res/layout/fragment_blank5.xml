<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/BG"
    tools:context=".Fragment.BlankFragment5">

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="92dp"
        android:layout_height="76dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/iconlogo" />

    <Button
        android:id="@+id/buttonBack10"
        android:layout_width="177dp"
        android:layout_height="55dp"
        android:layout_marginTop="192dp"
        android:layout_marginBottom="67dp"
        android:backgroundTint="@color/BG"
        android:text="@string/ButtonBack"
        android:textColor="@color/NameButton"
        android:textSize="24sp"
        app:cornerRadius="9sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonObratKlaponNo"
        app:strokeColor="@color/BGButton"
        app:strokeWidth="1sp" />

    <Button
        android:id="@+id/buttonObratKlaponNo"
        android:layout_width="177dp"
        android:layout_height="55dp"
        android:layout_marginTop="31dp"
        android:backgroundTint="@color/BGButton"
        android:text="@string/ButtonNo"
        android:textSize="24dp"
        app:cornerRadius="9sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonObratKlaponYes"
        app:strokeColor="@color/StrokeButton"
        app:strokeWidth="1dp" />

    <Button
        android:id="@+id/buttonObratKlaponYes"
        android:layout_width="177dp"
        android:layout_height="55dp"
        app:strokeColor="@color/StrokeButton"
        app:strokeWidth="1sp"
        android:textSize="24dp"
        app:cornerRadius="9sp"
        android:backgroundTint="@color/BGButton"
        android:layout_marginStart="161dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="162dp"
        android:text="@string/ButtonYes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ObratKlapon" />

    <TextView
        android:id="@+id/ObratKlapon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="176dp"
        android:layout_marginTop="85dp"
        android:layout_marginEnd="177dp"
        android:text="Обратный клапан"
        android:textColor="@color/ColorQuestion"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="88dp"
        android:text="Выбор"
        android:textColor="@color/ColorQuestion"
        android:textSize="32sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>